{
  "name": "deric-cdh",
  "version": "0.1.0",
  "author": "Tomas Barton",
  "summary": "Module for managing Cloudera Hadoop Distribution",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-cdh",
  "project_page": "https://github.com/deric/puppet-cdh",
  "issues_url": "https://github.com/deric/puppet-cdh/issues",
  "description": "Puppet module for Hadoop and the rest of Cloudera's CDH 5.",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 2.3.3"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "10.04",
        "12.04",
        "14.04"
      ]
    }
  ]
}
